  "bugs": {
    "url": "https://github.com/mrkhachaturov/hae-vault/issues"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "better-sqlite3": "^12.6.2",
    "commander": "^12.1.0",
    "dotenv": "^17.3.1",
    "express": "^4.21.0"
  },
